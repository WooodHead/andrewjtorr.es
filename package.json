{
  "name": "andrewjtorr.es",
  "version": "0.0.0",
  "description": "Personal website of Andrew Torres",
  "keywords": [
    "personal-website",
    "website"
  ],
  "homepage": "https://github.com/ajtorres9/andrewjtorr.es#readme",
  "bugs": "https://github.com/ajtorres9/andrewjtorr.es/issues",
  "license": "MIT",
  "author": "Andrew Torres <andrew.jonathan.torres@gmail.com> (https://andrewjtorr.es)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajtorres9/andrewjtorr.es.git"
  },
  "scripts": {
    "build-stats": "yarn build --analyze --release",
    "build": "webpack --config config/webpack.config.js",
    "lint": "yarn lint-scripts && yarn lint-styles && yarn lint-types",
    "lint-scripts": "eslint --cache --ignore-path .gitignore --ignore-pattern '!**/.*' '**/*.{js,ts,tsx}'",
    "lint-styles": "stylelint --cache --ignore-path .gitignore --ignore-pattern '!**/.*' '**/*.{ts,tsx}'",
    "lint-types": "tsc --noEmit",
    "start": "webpack-dev-server --config config/webpack.config.js",
    "test": "yarn test-units",
    "test-units": "jest"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loadable/component": "^5.6.0",
    "@reach/router": "^1.2.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@svgr/webpack": "^4.1.0",
    "@types/jest": "^24.0.0",
    "@types/loadable__component": "^5.2.0",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/styled-components": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.2.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "codecov": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-circus": "^24.1.0",
    "jest-config": "^24.1.0",
    "jest-dom": "^3.0.2",
    "jest-junit": "^6.2.1",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.3",
    "polished": "^2.3.3",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.5.3",
    "semantic-release": "^15.13.3",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-junit-formatter": "^0.2.1",
    "stylelint-order": "^2.0.0",
    "stylelint-processor-styled-components": "^1.5.2",
    "terser-webpack-plugin": "^1.2.2",
    "typescript": "~3.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "engines": {
    "node": ">=10.15.1",
    "yarn": ">=1.13.0"
  },
  "private": true,
  "browserslist": [
    "> 0.20%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "eslint --fix",
        "stylelint",
        "jest --bail --findRelatedTests",
        "git add"
      ],
      "*.{json,md,yml}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
